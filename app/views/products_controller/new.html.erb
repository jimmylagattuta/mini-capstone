<h1> New Pet </h1>
<%= form_tag "/products", method: :post do %>
<br>
	Breed: <input type="text" name="product_name">
<br>
	Price: <input type="text" name="product_price">
<br>	
	Description: <input type="text" name="product_description">
<br>
	In-Stock: <input type="text" name="product_stock">
<br>
	Weight: <input type="text" name="product_weight">
<br>
	Supplier ID: <input type="text" name="product_supplier_id">
<br>
	User ID: <input type="text" name="product_user_id">
<br>
	<input type="submit" value="create_product">
<% end %>